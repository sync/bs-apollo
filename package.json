{
  "name": "@dblechoc/bs-apollo",
  "version": "2.2.0",
  "description": "Minimal @apollo/react-hooks bindings written in ReasonML",
  "main": "src/ApolloHooks.re",
  "author": "Anthony Mittaz",
  "license": "MIT",
  "scripts": {
    "build": "bsb -make-world",
    "dev": "bsb -make-world -w",
    "clean": "bsb -clean-world",
    "test": "yarn build && jest",
    "test-watch": "jest -o --watch",
    "format": "bsrefmt --in-place src/*.r*",
    "preversion": "yarn clean && yarn format && yarn test"
  },
  "files": [
    "src",
    "dist",
    "bsconfig.json"
  ],
  "keywords": [
    "react",
    "reason",
    "reason-react",
    "reasonml",
    "ocaml",
    "bucklescript",
    "apollo",
    "graphql",
    "hooks"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sync/bs-apollo.git"
  },
  "peerDependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@baransu/graphql_ppx_re": "^0.3.2",
    "apollo-client": "^2.6.4",
    "apollo-utilities": "^1.3.2",
    "bs-platform": "^5.0.6 || ^6.2.1 || ^7.0.0",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "reason-react": "^0.7.0"
  },
  "devDependencies": {
    "@apollo/react-hooks": "3.1.3",
    "@baransu/graphql_ppx_re": "0.3.2",
    "@glennsl/bs-jest": "0.4.9",
    "@types/react": "16.9.11",
    "apollo-boost": "0.4.4",
    "apollo-client": "2.6.4",
    "apollo-utilities": "1.3.2",
    "bs-dom-testing-library": "0.5.1",
    "bs-platform": "6.2.1",
    "bs-react-testing-library": "0.6.0",
    "graphql": "14.5.8",
    "graphql-tag": "2.10.1",
    "jest-fetch-mock": "2.1.2",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "reason-react": "0.7.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "testMatch": [
      "**/lib/js/__tests__/*_test.bs.js"
    ],
    "automock": false,
    "setupFiles": [
      "./setupJest.js"
    ]
  }
}
